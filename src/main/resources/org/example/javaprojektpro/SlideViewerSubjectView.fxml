            <Button fx:id="soButton" mnemonicParsing="false" onAction="#handleSoButton" prefHeight="319.0" prefWidth="451.0" text="Systemy Operacyjne" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button layoutX="14.0" layoutY="362.0" mnemonicParsing="false" onAction="#handleBackToMainWindow" text="Do menu głównego" />
   </children>
</AnchorPane>
